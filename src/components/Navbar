// components/Navbar.js
import { useAuth } from '../lib/useAuth';

export default function Navbar() {
  const user = useAuth();
  return (
    <nav>
      {user?.permission === 'all' && <a href="/admin">Admin Panel</a>}
      {['all','editor'].includes(user?.permission) && <a href="/citas">Citas</a>}
      <a href="/pacientes">Pacientes</a>
    </nav>
  );
}
